#include "main.h"

/**
 * times_table - prints the 9 times table.
 */
void times_table(void)
{
    int row, col, result;

    for (row = 0; row <= 9; row++)
{
        for (col = 0; col <= 9; col++)
{
            result = row * col;

            if (col == 0)
{
                _putchar(result + '0');
}
            if
{
	{        
       	_putchar(',');
	}     

                 else if (result < 10)
                 
{
                    _putchar((result / 10) + '0');

                _putchar((result % 10) + '0');
}

            if (col < 9)
                _putchar(',');
	    
}
}
}
_putchar('\n');
}

